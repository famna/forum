<h1>Sign In</h1>

<%= form_with url: sign_in_path,data: { turbo: false } do |form|%>
 <div class="mb3">
  <%= form.label :email%>
  <%= form.text_field :email, class: "form-control", placeholder: "qwerty@gmail.com" %>
 </div>

 <div class="mb3">
  <%= form.label :password%>
  <%= form.password_field :password, class: "form-control", placeholder: "password"%>
 </div class="mb3">

 <div class="mb3">
 <%= form.submit "Sign Up", class: "btn btn-primary"%>
 </div>
<% end %>