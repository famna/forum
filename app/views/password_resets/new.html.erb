<h1>Forgot your password</h1>
<%= form_with url: password_reset_path do |form| %>

 <div class="mb3">
  <%= form.label :email%>
  <%= form.text_field :email, class: "form-control", placeholder: "qwerty@gmail.com" %>
 </div>

 <div class="mb3">
 <%= form.submit "Reset Password", class: "btn btn-primary"%>
 </div>
<%end%>